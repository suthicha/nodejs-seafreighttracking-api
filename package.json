{
  "name": "nodejs-seafreighttracking-api",
  "version": "1.0.0",
  "description": "an api project interface to mssql database",
  "main": "dist",
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --preset es2015,stage-0",
    "start": "NODE_ENV=production pm2 start build",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suthicha/nodejs-seafreighttracking-api.git"
  },
  "author": "Suthicha Poonakaow",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/suthicha/nodejs-seafreighttracking-api/issues"
  },
  "homepage": "https://github.com/suthicha/nodejs-seafreighttracking-api#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "jsonwebtoken": "^7.4.1",
    "mssql": "^2.3.1",
    "squel": "^5.11.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.2.0",
    "nodemon": "^1.11.0"
  }
}
